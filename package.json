{
  "name": "doto",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:local": "expo start --env-file .env.local",
    "start:prod": "expo start --env-file .env",
    "android": "expo run:android",
    "android:local": "expo start --android --env-file .env.local",
    "ios": "expo run:ios",
    "ios:local": "expo start --ios --env-file .env.local",
    "web": "expo start --web",
    "web:local": "expo start --web --env-file .env.local",
    "build:apk": "eas build --profile preview --platform android",
    "build:apk:dev": "eas build --profile development --platform android",
    "build:ios": "eas build --profile production --platform ios",
    "build:ios:dev": "eas build --profile development --platform ios"
  },
  "dependencies": {
    "@instantdb/react-native": "^0.17.6",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/stack": "^6.3.29",
    "expo": "~51.0.0",
    "expo-auth-session": "~5.5.2",
    "expo-crypto": "~13.0.2",
    "expo-image-picker": "~15.0.7",
    "expo-location": "~17.0.1",
    "expo-notifications": "~0.28.0",
    "expo-status-bar": "~1.12.1",
    "expo-web-browser": "~13.0.3",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-get-random-values": "~1.11.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0"
  },
  "private": true
}
