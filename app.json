{
  "expo": {
    "name": "DOTO",
    "slug": "doto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "doto",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#DC2626"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.doto.app",
      "usesAppleSignIn": false,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "DOTO needs your location to show nearby tasks.",
        "NSPhotoLibraryUsageDescription": "DOTO needs access to your photos to upload images.",
        "NSCameraUsageDescription": "DOTO needs camera access to take photos.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["doto", "fb2301357543703221"]
          }
        ]
      }
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#DC2626",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} new notifications"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#DC2626"
      },
      "package": "com.doto.app",
      "versionCode": 6,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "doto"
            },
            {
              "scheme": "fb2301357543703221"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "facebookScheme": "fb2301357543703221",
    "facebookAppId": "2301357543703221",
    "facebookDisplayName": "DOTO",
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow DOTO to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow DOTO to access your photos.",
          "cameraPermission": "Allow DOTO to access your camera."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#DC2626",
          "sounds": []
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5519a148-f81d-466e-8fe0-a3557958c204"
      },
      "googleClientId": "{YOUR_GOOGLE_CLIENT_ID}",
      "googleClientIdIos": "{YOUR_GOOGLE_IOS_CLIENT_ID}",
      "googleClientIdAndroid": "{YOUR_GOOGLE_ANDROID_CLIENT_ID}",
      "facebookAppId": "2301357543703221"
    }
  }
}
